{
  "contributions": {
    "settings": [
      {
        "file_patterns": [
          "/LSP-Roslyn.sublime-settings"
        ],
        "schema": {
          "$id": "sublime://settings/LSP-Roslyn",
          "allOf": [
            {
              "$ref": "sublime://settings/LSP-plugin-base"
            },
            {
              "$ref": "sublime://settings/LSP-Roslyn#/definitions/PluginConfig"
            }
          ],
          "definitions": {
            "PluginConfig": {
              "properties": {
                "settings": {
                  "additionalProperties": false,
                  "properties": {
                    "roslyn.defaultLaunchSolution": {
                      "default": null,
                      "description": "The name of the default solution used at start up if the repo has multiple solutions. e.g. 'MyAwesomeSolution.sln'. Default value is `null` which will cause the first in alphabetical order to be chosen.",
                      "type": ["string", "null"]
                    },
                    "roslyn.backgroundAnalysis": {
                      "type": "object",
                      "description": "Background analysis settings for diagnostics",
                      "properties": {
                        "dotnet_analyzer_diagnostics_scope": {
                          "type": "string",
                          "enum": ["openFiles", "fullSolution", "none"],
                          "default": "openFiles",
                          "description": "Scope of the background analysis for .NET analyzer diagnostics"
                        },
                        "dotnet_compiler_diagnostics_scope": {
                          "type": "string",
                          "enum": ["openFiles", "fullSolution", "none"],
                          "default": "openFiles",
                          "description": "Scope of the background analysis for .NET compiler diagnostics"
                        }
                      }
                    },
                    "roslyn.codeLens": {
                      "type": "object",
                      "description": "Code lens settings",
                      "properties": {
                        "dotnet_enable_references_code_lens": {
                          "type": "boolean",
                          "default": true,
                          "description": "Enable code lens references"
                        },
                        "dotnet_enable_tests_code_lens": {
                          "type": "boolean",
                          "default": true,
                          "description": "Enable tests code lens"
                        }
                      }
                    },
                    "roslyn.completion": {
                      "type": "object",
                      "description": "Completion settings",
                      "properties": {
                        "dotnet_provide_regex_completions": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show regular expressions in completion list"
                        },
                        "dotnet_show_completion_items_from_unimported_namespaces": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show unimported types and extension methods in completion lists"
                        },
                        "dotnet_show_name_completion_suggestions": {
                          "type": "boolean",
                          "default": true,
                          "description": "Perform automatic object name completion"
                        }
                      }
                    },
                    "roslyn.inlayHints": {
                      "type": "object",
                      "description": "Inlay hints settings",
                      "properties": {
                        "csharp_enable_inlay_hints_for_implicit_object_creation": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for implicit object creation"
                        },
                        "csharp_enable_inlay_hints_for_implicit_variable_types": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for variables with inferred types"
                        },
                        "csharp_enable_inlay_hints_for_lambda_parameter_types": {
                          "type": "boolean",
                          "default": false,
                          "description": "Show hints for lambda parameter types"
                        },
                        "csharp_enable_inlay_hints_for_types": {
                          "type": "boolean",
                          "default": true,
                          "description": "Display inline type hints"
                        },
                        "dotnet_enable_inlay_hints_for_indexer_parameters": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for indexers"
                        },
                        "dotnet_enable_inlay_hints_for_literal_parameters": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for literals"
                        },
                        "dotnet_enable_inlay_hints_for_object_creation_parameters": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for 'new' expressions"
                        },
                        "dotnet_enable_inlay_hints_for_other_parameters": {
                          "type": "boolean",
                          "default": true,
                          "description": "Show hints for everything else"
                        },
                        "dotnet_enable_inlay_hints_for_parameters": {
                          "type": "boolean",
                          "default": true,
                          "description": "Display inline parameter name hints"
                        },
                        "dotnet_suppress_inlay_hints_for_parameters_that_differ_only_by_suffix": {
                          "type": "boolean",
                          "default": true,
                          "description": "Suppress hints when parameter names differ only by suffix"
                        },
                        "dotnet_suppress_inlay_hints_for_parameters_that_match_argument_name": {
                          "type": "boolean",
                          "default": true,
                          "description": "Suppress hints when argument matches parameter name"
                        },
                        "dotnet_suppress_inlay_hints_for_parameters_that_match_method_intent": {
                          "type": "boolean",
                          "default": true,
                          "description": "Suppress hints when parameter name matches the method's intent"
                        }
                      }
                    },
                    "roslyn.symbolSearch": {
                      "type": "object",
                      "description": "Symbol search settings",
                      "properties": {
                        "dotnet_search_reference_assemblies": {
                          "type": "boolean",
                          "default": true,
                          "description": "Search symbols in reference assemblies"
                        }
                      }
                    },
                    "roslyn.formatting": {
                      "type": "object",
                      "description": "Formatting settings",
                      "properties": {
                        "dotnet_organize_imports_on_format": {
                          "type": "boolean",
                          "default": false,
                          "description": "Sort using directives on format alphabetically"
                        }
                      }
                    },
                    "roslyn.loggingLevel": {
                      "type": "string",
                      "enum": ["Trace", "Debug", "Information", "Warning", "Error", "Critical", "None"],
                      "default": "Information",
                      "description": "Logging level for the Roslyn language server"
                    },
                    "roslyn.enableAnalyzers": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable Roslyn analyzers support"
                    },
                    "roslyn.loadProjectsOnDemand": {
                      "type": "boolean",
                      "default": false,
                      "description": "Load projects on demand (useful for large solutions)"
                    }
                  }
                }
              }
            }
          }
        }
      },
      {
        "file_patterns": [
          "/*.sublime-project"
        ],
        "schema": {
          "properties": {
            "settings": {
              "properties": {
                "LSP": {
                  "properties": {
                    "Roslyn": {
                      "$ref": "sublime://settings/LSP-Roslyn#/definitions/PluginConfig"
                    }
                  }
                }
              }
            }
          }
        }
      }
    ]
  }
}
